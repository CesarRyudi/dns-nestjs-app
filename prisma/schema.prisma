generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Testes {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  content1  String
  content2  String
  content3  String
  numer1    Int
  numer2    Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

